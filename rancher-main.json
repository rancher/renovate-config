{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": ["{{#if env.RENOVATE_EXTENDS_PRESET}}{{env.RENOVATE_EXTENDS_PRESET}}{{else}}github>rancher/renovate-config#release{{/if}}"],
  "packageRules": [
    {
      "matchPackageNames": [
        "golang",
        "go",
        "registry.suse.com/bci/golang",
        "registry.opensuse.org/opensuse/bci/golang"
      ],
      "allowedVersions": "<1.26.0"
    },
    {
      "matchDatasources": [
        "golang-version"
      ],
      "allowedVersions": "<1.26.0"
    },
    {
      "matchManagers": [
        "gomod",
        "custom.regex",
        "dockerfile"
      ],
      "matchPackageNames": [
        "rancher/kubectl",
        "kubernetes/kubernetes",
        "k8s.io/kubernetes"
      ],
      "allowedVersions": "<1.36.0"
    },
    {
      "matchManagers": [
        "gomod"
      ],
      "matchPackageNames": [
        "!k8s.io/kubernetes",
        "!k8s.io/gengo",
        "!k8s.io/gengo/**",
        "!k8s.io/utils",
        "!k8s.io/utils/**",
        "!k8s.io/kube-openapi",
        "!k8s.io/kube-openapi/**",
        "k8s.io/**"
      ],
      "allowedVersions": "<0.36.0"
    }
  ]
}
